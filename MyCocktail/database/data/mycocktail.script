INSERT INTO MASHUPS VALUES(45,'Bar Char Blogs','Statistic which shows the number of blog per user','{"name":"Bar Char Blogs","requires":{"Services.Romulus":"http://localhost:8084/MyCocktail/js/afrous/packages/romulus.js","Renderer.Plotr":"http://localhost:8084/MyCocktail/js/afrous/renderers/plotr-afrous.js"},"params":[],"output":"${bar}","actions":[{"type":"Services.Romulus.RomulusBlog","name":"romulusblog","inputs":{"option":"Statistics","statistic":"User"}},{"type":"Renderer.Plotr.Bar","name":"bar","inputs":{"records":"${romulusblog.blogs}","labelField":"author","valueField1":"number","width":"300","height":"300","colorScheme":"green"}}]}')
INSERT INTO MASHUPS VALUES(46,'Blog users in GMaps','It shows the users of the blog in a map. ','{"name":"Blog users in GMaps","requires":{"Services.Romulus":"http://localhost:8084/MyCocktail/js/afrous/packages/romulus.js","Renderer.GoogleMaps":"http://localhost:8084/MyCocktail/js/afrous/renderers/google-maps.js"},"params":[],"output":"${googlemapsmarker}","actions":[{"type":"Services.Romulus.RomulusBlog","name":"romulusblog","inputs":{"option":"Users Addresses","statistic":"User"}},{"type":"Renderer.GoogleMaps.GoogleMapsMarker","name":"googlemapsmarker","inputs":{"records":"${romulusblog.blogs}","labelField":"user","valueField":"address","width":"800","height":"300","zoomController":"true","mapTypeController":"true","mapType":"Hibrid"}}]}')
