package com.germinus.mashupbuilder.services;

import java.util.List;
import java.util.ArrayList;

/**
 * This class represents a UnitActionPackage in MyCocktail application.
 * A UnitActionPackage is a set of operators, services or renderes which
 * can be used in MyCocktail to make operation with the data, retrived data
 * from services or display the data. An element of this set is called
 * UnitAction.
 * @author David del Pozo González
 */
public class UnitActionPackage {
    /**
     * The name of the UnitActionPackage.
     */
    private String name;
    /**
     * The label of the UnitActionPackage.
     */
    private String label;
     /**
     * The URL where is placed the icon of the UnitActionPackage.
     */
    private String icon = "";
    /**
     * The base URL the UnitActionPackage.
     */
    private String baseURL;
    /**
     * The list of the UnitActions.
     */
    private List<UnitAction> unitActions = new ArrayList<UnitAction>();
    /**
     * The URL where is published this library.
     */
    private String libraryURL;

    /**
     * Gets the name of the UnitActionPackage.
     * @return the name of the UnitActionPackage
     */
    public final String getName() {
        return name;
    }

    /**
     * Sets the name of the UnitActionPackage.
     * @param name the name to set of the UnitActionPackage
     */
    public final void setName(final String name) {
        this.name = name;
    }

    /**
     * Gets the label of the UnitActionPackage.
     * @return the label of the UnitActionPackage
     */
    public final String getLabel() {
        return label;
    }

    /**
     * Sets the label of the UnitActionPackage.
     * @param label the label to set of the UnitActionPackage
     */
    public final void setLabel(final String label) {
        this.label = label;
    }

    /**
     * Gets the URL where is placed the icon of the UnitActionPackage.
     * @return the URL where is placed the icon of the UnitActionPackage
     */
    public final String getIcon() {
        return icon;
    }

    /**
     * Sets the URL where is placed the icon of the UnitActionPackage.
     * @param icon the URL where is placed the icon of the UnitActionPackage
     */
    public final void setIcon(final String icon) {
        this.icon = icon;
    }

    /**
     * Gets the base URL the UnitActionPackage.
     * @return the baseURL
     */
    public final String getBaseURL() {
        return baseURL;
    }

    /**
     * Sets the base URL the UnitActionPackage.
     * @param baseURL the baseURL to set
     */
    public final void setBaseURL(final String baseURL) {
        this.baseURL = baseURL;
    }

    /**
     * Gets the list of the unitActions.
     * @return the unitActions
     */
    public final List<UnitAction> getUnitActions() {
        return unitActions;
    }

    /**
     * Sets the list of the unitActions.
     * @param unitActions the unitActions to set
     */
    public final void setUnitActions(final List<UnitAction> unitActions) {
        this.unitActions = unitActions;
    }

    /**
     * Gets the URL where is published this library.
     * @return the URL where is published this library
     */
    public final String getLibraryURL() {
        return libraryURL;
    }

    /**
     * Sets the URL where is published this library.
     * @param libraryURL the URL where is published this library
     */
    public final void setLibraryURL(final String libraryURL) {
        this.libraryURL = libraryURL;
    }
}
